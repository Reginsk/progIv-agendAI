{
  "name": "minirutter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "start": "cd ./dist && node src/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --ignore node_modules src/server.ts",
    "db:migrate": "yarn run typeorm migration:run -- -d src/database/data-source.ts",
    "db:revert": "yarn run typeorm migration:revert -- -d src/database/data-source.ts",
    "db:sync": "yarn run typeorm schema:sync -d src/database/data-source.ts",
    "lint": "eslint src/**/*.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bull-board/express": "^6.0.0",
    "@sentry/node": "^8.33.1",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.2",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "http-graceful-shutdown": "^3.1.13",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^16.11.10",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.2"
  }
}
